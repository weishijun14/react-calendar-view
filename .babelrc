{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": ["last 2 versions"],
            "node": "current"
          },
          "debug": false
        }
      ],
       "@babel/preset-react",
    ],
    "env": {
      "development": {
        "presets": [["@babel/preset-react", { "development": true }]]
      }
    },
    "plugins": ["@babel/plugin-transform-runtime"],
    "minified": false
  }